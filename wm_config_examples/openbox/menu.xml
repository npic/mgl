<?xml version="1.0" encoding="UTF-8"?>

<openbox_menu xmlns="http://openbox.org/"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://openbox.org/
                file:///usr/share/openbox/menu.xsd">

<menu id="root-menu" label="Openbox 3">
  <item label="My Game Library">
    <action name="Execute"><execute>/usr/bin/urxvt +hold -sr -fg gray50 -bg black -e /mnt/data/games/MGL/mgl.pl</execute></action>
  </item>
  <item label="Terminal emulator">
    <action name="Execute"><execute>/usr/bin/urxvt +hold -sr -fg gray50 -bg black</execute></action>
  </item>
  <item label="Exit">
    <action name="Exit">
        <prompt>no</prompt>
    </action>
  </item>
</menu>

</openbox_menu>
